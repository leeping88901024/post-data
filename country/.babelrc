{
   "presets": [
      [
        "env", {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
       ["transform-es2015-arrow-functions"],
       ["babel-plugin-transform-es2015-destructuring"],
       ["babel-plugin-transform-es2015-spread"],
       ["babel-plugin-transform-object-rest-spread"],
       ["transform-runtime", {
        "polyfill": false,
        "regenerator": true
      }]
     ]
  }
  